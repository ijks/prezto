# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

# Autocorrect is annoying...
unsetopt CORRECT

# assorted aliases
alias py=python3
alias ipy=ipython
alias bpy==bpython
alias st='subl -a'
alias stf='subl -n .'
alias stn='subl -n'
alias startmc='~/mcserv/Direwolf20/ServerStart.sh'
alias wo=workon
alias de=deactivate
alias gst='git status'
